# Telegram API 설정
# https://my.telegram.org 에서 발급
API_ID=your_api_id
API_HASH=your_api_hash
PHONE=+821012345678

# 기본 대상 채팅방 ID (선택)
# CLI에서 --chat-id로 오버라이드 가능
DEFAULT_CHAT_ID=

# 데이터베이스 경로 (선택, 기본값: ./search.db)
DB_PATH=./search.db

# Supabase 설정
# https://supabase.com 에서 프로젝트 생성 후 발급
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_KEY=your_service_key
